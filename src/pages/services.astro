---
import Layout from "../layouts/Layout.astro";
import Showcase from "../components/Showcase.astro";
import Navbar from "../components/Navbar.astro";
import ListItems from "../components/ListItems.astro";
import ContactForm from "../components/ContactForm.astro";
---
<Layout>
  <Navbar 
    showcase={true}
  />
  <Showcase 
    showcase={true}
    title="Our Services"
  content="View some of the services we provide."
  image="https://images.unsplash.com/photo-1706988056350-d112eabb6835?q=80&w=1170&auto=format&fit=crop&ixlib=rb-4.1.0&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D"
  showcaseLink="/services"
  />
  <ListItems />
  <ContactForm />
</Layout>